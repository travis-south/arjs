{
  "name": "arjs",
  "version": "0.1.0",
  "description": "playground",
  "main": "src/server.js",
  "repository": "git@github.com:travis-south/arjs.git",
  "author": "Irvin Capagcuan",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --serial expose-server server-logs webpack test",
    "expose-server": "pm2 start src/server.js --watch --interpreter babel-node -f",
    "server-logs": "pm2 logs >/dev/stdout 2>&1 &",
    "webpack": "webpack -wd >/dev/stdout 2>&1 &",
    "test": "jest --watch"
  },
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ]
  },
  "private": false,
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "react-test-renderer": "^16.5.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "pm2": "^3.1.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "webpack": "^4.19.0"
  }
}
